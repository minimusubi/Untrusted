if (player.atLocation(map.getWidth() - 2, map.getHeight() - 1))
            	me.move("down");
            else if (player.atLocation(map.getWidth() - 2, map.getHeight() - 3))
            	me.move("up");
            else if (player.atLocation(map.getWidth() - 3, map.getHeight() - 2))
            	me.move("left");
            else if (player.atLocation(map.getWidth() - 1, map.getHeight() - 2))
            	me.move("right");
